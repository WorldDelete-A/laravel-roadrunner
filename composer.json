  "require": {
    "php": "^8.0",
    "illuminate/console": "^5.6|^6|^7|^8",
    "illuminate/contracts": "^5.6|^6|^7|^8",
    "illuminate/http": "^5.6|^6|^7|^8",
    "illuminate/routing": "^5.6|^6|^7|^8",
    "illuminate/support": "^5.6|^6|^7|^8",
    "symfony/psr-http-message-bridge": "^1.3",
    "spiral/roadrunner": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hunternnm\\LaravelRoadrunner\\RoadrunnerServiceProvider"
      ]
    }
  },
  "autoload": {
